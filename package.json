    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "password-validator": "^5.3.0",
    "pg": "^8.11.1",
